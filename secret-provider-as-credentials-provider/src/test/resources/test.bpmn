<bpmn:definitions xmlns:bpmn="http://www.omg.org/spec/BPMN/20100524/MODEL" xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI" xmlns:dc="http://www.omg.org/spec/DD/20100524/DC" xmlns:zeebe="http://camunda.org/schema/zeebe/1.0" xmlns:di="http://www.omg.org/spec/DD/20100524/DI" xmlns:modeler="http://camunda.org/schema/modeler/1.0" id="Definitions_1p4kr34" targetNamespace="http://bpmn.io/schema/bpmn" exporter="Camunda Modeler" exporterVersion="5.34.0" modeler:executionPlatform="Camunda Cloud" modeler:executionPlatformVersion="8.7.0">
  <bpmn:process id="ZeebeTopologyFetcherProcess" name="Zeebe Topology Fetcher" isExecutable="true">
    <bpmn:startEvent id="ZeebeTopologyRequiredStartEvent" name="Zeebe Topology required">
      <bpmn:extensionElements>
        <zeebe:ioMapping>
          <zeebe:output source="=if is defined(zeebeTopologyUrl) then zeebeTopologyUrl else &#34;http://localhost:8088/v2/topology&#34;" target="zeebeTopologyUrl" />
        </zeebe:ioMapping>
      </bpmn:extensionElements>
      <bpmn:outgoing>Flow_06zpd2y</bpmn:outgoing>
    </bpmn:startEvent>
    <bpmn:sequenceFlow id="Flow_06zpd2y" sourceRef="ZeebeTopologyRequiredStartEvent" targetRef="GetZeebeTopologyTask" />
    <bpmn:serviceTask id="GetZeebeTopologyTask" name="Get Zeebe Topology" zeebe:modelerTemplate="io.camunda.connectors.HttpJson.v2" zeebe:modelerTemplateVersion="10" zeebe:modelerTemplateIcon="data:image/svg+xml;base64,">
      <bpmn:extensionElements>
        <zeebe:taskDefinition type="io.camunda:http-json:1" retries="3" />
        <zeebe:ioMapping>
          <zeebe:input source="bearer" target="authentication.type" />
          <zeebe:input source="{{secrets.bearerToken(zeebe)}}" target="authentication.token" />
          <zeebe:input source="GET" target="method" />
          <zeebe:input source="=zeebeTopologyUrl" target="url" />
          <zeebe:input source="=false" target="storeResponse" />
          <zeebe:input source="=20" target="connectionTimeoutInSeconds" />
          <zeebe:input source="=20" target="readTimeoutInSeconds" />
          <zeebe:input source="=false" target="ignoreNullValues" />
        </zeebe:ioMapping>
        <zeebe:taskHeaders>
          <zeebe:header key="resultExpression" value="={&#10;  topology: response.body&#10;}" />
          <zeebe:header key="retryBackoff" value="PT0S" />
        </zeebe:taskHeaders>
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_06zpd2y</bpmn:incoming>
      <bpmn:outgoing>Flow_1342s5b</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:endEvent id="ZeebeTopologyDeliveredEndEvent" name="Zeebe topology delivered">
      <bpmn:incoming>Flow_1342s5b</bpmn:incoming>
    </bpmn:endEvent>
    <bpmn:sequenceFlow id="Flow_1342s5b" sourceRef="GetZeebeTopologyTask" targetRef="ZeebeTopologyDeliveredEndEvent" />
  </bpmn:process>
